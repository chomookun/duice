{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es6",
    "strict": true,
    "strictNullChecks": false,
    "noImplicitThis": false,
    "module":"none",
    "removeComments": true,
    "allowJs": true,
    "watch": false,
  },
  "typedocOptions":{
    "mode": "modules",
    "out": "doc",
    "theme": "default"
  },
  "include": [
    "./src/**/*.ts"
  ],
}
